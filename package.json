{
  "name": "blob-store-lab",
  "version": "0.0.1",
  "description": "lab for blob storage of different types",
  "main": "index.js",
  "files": [
    "**/*.ts"
  ],
  "scripts": {
    "start": "./node_modules/.bin/tsc index.ts && node -r esm index.js",
    "test": "jest *test.ts"
  },
  "dependencies": {
    "abstract-blob-store": "^3.3.5",
    "aws-sdk": "^2.917.0",
    "fs-blob-store": "^6.0.0",
    "s3-blob-store": "^4.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".*": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "license": "MIT"
}
